<%- include("partials/header"); -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(post => { %>
  <h1><%= post.title %></h1>

  <% if (post.body.length > 100) { %>
    <p><%= post.body.slice(0, 100) + "..."; %>
    <a href=<%= post.link %>>Read More</a></p>
  <% } else { %>
  <p><%= post.body %> </p>
  <% } %>
<% });  %>
  
<%- include("partials/footer"); -%>